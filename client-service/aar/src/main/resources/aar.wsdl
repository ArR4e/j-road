<?xml version="1.0" encoding="utf-8"?>
<definitions targetNamespace="http://producers.aar.xtee.riik.ee/producer/aar"
             xmlns="http://schemas.xmlsoap.org/wsdl/"
             xmlns:xtee="http://x-tee.riik.ee/xsd/xtee.xsd"
             xmlns:xs="http://www.w3.org/2001/XMLSchema"
             xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
             xmlns:mime="http://schemas.xmlsoap.org/wsdl/mime/"
             xmlns:tns="http://producers.aar.xtee.riik.ee/producer/aar"
             xmlns:SOAP-ENC="http://schemas.xmlsoap.org/soap/encoding/"
             xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/">
  <types>
    <xs:schema targetNamespace="http://producers.aar.xtee.riik.ee/producer/aar">
      <xs:import namespace="http://x-tee.riik.ee/xsd/xtee.xsd"
                 schemaLocation="http://x-tee.riik.ee/xsd/xtee.xsd" />
      <xs:import namespace="http://schemas.xmlsoap.org/soap/encoding/"
                 schemaLocation="http://schemas.xmlsoap.org/soap/encoding/" />

      <xs:complexType name="oigusedParing">
        <xs:all>
          <xs:element name="registrikood" type="xs:string" minOccurs="0">

            <xs:annotation>
              <xs:appinfo>
                <xtee:title>Asutuse registrikood (Õigus)</xtee:title>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="ametikohtId" type="xs:integer" minOccurs="0">
            <xs:annotation>

              <xs:appinfo>
                <xtee:title>Ametikoha ID</xtee:title>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="gruppId" type="xs:integer" minOccurs="0">
            <xs:annotation>
              <xs:appinfo>

                <xtee:title>Grupi ID</xtee:title>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="isikukood" type="xs:string" minOccurs="0">
            <xs:annotation>
              <xs:appinfo>
                <xtee:title>Isikukood</xtee:title>

              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:all>
      </xs:complexType>

      <!-- Grupi õiguste funktsionaalsus ei ole hetkel realiseeritud.
           Õiguse sügavusega ei ole hetkel arvestatud.
           Sisendi ning väljundi registrikood kuulub õigusega seotud asutusele. Ametikohaga seotus registrikood võib olla arvutatud ametikoha id väärtusest.
           Juhul, kui isikukood ei ole tühi, siis on väljastatud isikuga seotud õigused, kasutades filtritena "ametikoht_id" ning "registrikood".
           Juhul, kui isikukood ei ole tühi, ning teised väljad on tühjad, siis lisaks isiku õigustele tagastakse ka isiku ametikohad.
           Juhul, kui isikukood on tühi, ning ametikoht_id või registrikood ei ole tühjad, siis on on väljastatud kõik õigused, kasutades filtritena "ametikoht_id" ning "registrikood".
           Juhul, kui isikukood on tühi, ning oigusega on seotud õiguse piirang, siis vastavate isikukoodide komaga eraldatud nimekiri on väljastatud isikukood väljas. -->
      <xs:complexType name="oigusedVastus">
        <xs:all>

          <xs:element name="oigused" minOccurs="0">
            <xs:annotation>
              <xs:appinfo>
                <xtee:title>Õiguste jada</xtee:title>
              </xs:appinfo>
            </xs:annotation>
            <xs:complexType>
              <xs:complexContent>

                <xs:restriction base="SOAP-ENC:Array">
                  <xs:sequence>
                    <xs:element name="oigus" minOccurs="1" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:all>
                          <xs:element name="oigusNimi" type="xs:string" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                              <xs:appinfo>
                                <xtee:title>Õiguse nimetus</xtee:title>

                              </xs:appinfo>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="oigusAlates" type="xs:date" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                              <xs:appinfo>
                                <xtee:title>Õigus kehtib alates</xtee:title>
                              </xs:appinfo>

                            </xs:annotation>
                          </xs:element>
                          <xs:element name="oigusKuni" type="xs:date" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                              <xs:appinfo>
                                <xtee:title>Õigus kehtib kuni</xtee:title>
                              </xs:appinfo>
                            </xs:annotation>

                          </xs:element>
                          <xs:element name="grupp" type="xs:string" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                              <xs:appinfo>
                                <xtee:title>Grupp</xtee:title>
                              </xs:appinfo>
                            </xs:annotation>
                          </xs:element>

                          <xs:element name="registrikood" type="xs:string" minOccurs="1" maxOccurs="1">
                            <xs:annotation>
                              <xs:appinfo>
                                <xtee:title>Asutuse registrikood (Õigus)</xtee:title>
                              </xs:appinfo>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="ametikohtId" type="xs:integer" minOccurs="1" maxOccurs="1">

                            <xs:annotation>
                              <xs:appinfo>
                                <xtee:title>Ametikoha ID</xtee:title>
                              </xs:appinfo>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="isikukood" type="xs:string" minOccurs="0" maxOccurs="1">
                            <xs:annotation>

                              <xs:appinfo>
                                <xtee:title>Isikukood</xtee:title>
                              </xs:appinfo>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="ksAmetikohtId" type="xs:integer" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                              <xs:appinfo>

                                <xtee:title>Kõrgemalseisva ametikoha ID</xtee:title>
                              </xs:appinfo>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="ametikohtNimetus" type="xs:string" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                              <xs:appinfo>
                                <xtee:title>Ametikoha nimetus</xtee:title>

                              </xs:appinfo>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="ametikohtAlates" type="xs:date" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                              <xs:appinfo>
                                <xtee:title>Ametikoht kehtib alates</xtee:title>
                              </xs:appinfo>

                            </xs:annotation>
                          </xs:element>
                          <xs:element name="ametikohtKuni" type="xs:date" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                              <xs:appinfo>
                                <xtee:title>Ametikoht kehtib kuni</xtee:title>
                              </xs:appinfo>
                            </xs:annotation>

                          </xs:element>
                          <xs:element name="allyksusId" type="xs:integer" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                              <xs:appinfo>
                                <xtee:title>Allüksuse ID</xtee:title>
                              </xs:appinfo>
                            </xs:annotation>
                          </xs:element>

                        </xs:all>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:restriction>
              </xs:complexContent>
            </xs:complexType>
          </xs:element>
        </xs:all>

      </xs:complexType>

      <xs:complexType name="oigused2Paring">
        <xs:all>
          <xs:element name="oigusId" type="xs:integer" minOccurs="0">
            <xs:annotation>
              <xs:appinfo>
                <xtee:title>Õiguse ID</xtee:title>
              </xs:appinfo>

            </xs:annotation>
          </xs:element>
          <xs:element name="isikukood" type="xs:string" minOccurs="0">
            <xs:annotation>
              <xs:appinfo>
                <xtee:title>Isikukood</xtee:title>
              </xs:appinfo>
            </xs:annotation>

          </xs:element>
          <xs:element name="oigRegistrikood" type="xs:string" minOccurs="0">
            <xs:annotation>
              <xs:appinfo>
                <xtee:title>Asutuse registrikood (õigus)</xtee:title>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>

          <xs:element name="oigAsutusId" type="xs:integer" minOccurs="0">
            <xs:annotation>
              <xs:appinfo>
                <xtee:title>Asutuse ID (õigus)</xtee:title>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="ametikohtId" type="xs:integer" minOccurs="0">

            <xs:annotation>
              <xs:appinfo>
                <xtee:title>Ametikoha ID</xtee:title>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="akRegistrikood" type="xs:string" minOccurs="0">
            <xs:annotation>

              <xs:appinfo>
                <xtee:title>Asutuse registrikood (ametikoht)</xtee:title>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="akAsutusId" type="xs:integer" minOccurs="0">
            <xs:annotation>
              <xs:appinfo>

                <xtee:title>Asutuse ID (ametikoht)</xtee:title>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="limit" type="xs:integer" minOccurs="0">
            <xs:annotation>
              <xs:appinfo>
                <xtee:title>Vastuse limit</xtee:title>

              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="offset" type="xs:integer" minOccurs="0">
            <xs:annotation>
              <xs:appinfo>
                <xtee:title>Vastuse offset</xtee:title>
              </xs:appinfo>

            </xs:annotation>
          </xs:element>
        </xs:all>
      </xs:complexType>

      <!-- Õiguse sügavusega ei ole hetkel arvestatud
           Päringu vastuses on toodud kõik õigused mis vastavad sisendis toodud kriteerimitele (oiguse id, isikukood, ametikoha id,
           õigusega seotud asutuse id, ametikohaga seotud astuse id). Kui mõni õtsingu kriteerium on tühi, siis sellega ei arvestata.
           Asutuse id asemel saab anda asutuse registrikoodi. Mahiliste päringute puhul võib kasutada limit ja offset. Kui vastuses on
           vastuseOsa=true, siis vastus oli tagastatud osaliselt, ning ülejäänute andmete saamiseks tuleb päringu korrata määrates offset. -->
      <xs:complexType name="oigused2Vastus">
        <xs:all>
          <xs:element name="oigused" minOccurs="0">

            <xs:annotation>
              <xs:appinfo>
                <xtee:title>Õiguste jada</xtee:title>
              </xs:appinfo>
            </xs:annotation>
            <xs:complexType>
              <xs:complexContent>
                <xs:restriction base="SOAP-ENC:Array">

                  <xs:sequence>
                    <xs:element name="oigus" minOccurs="1" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:all>
                          <xs:element name="oigusId" type="xs:integer" minOccurs="1">
                            <xs:annotation>
                              <xs:appinfo>
                                <xtee:title>Õiguse ID</xtee:title>

                              </xs:appinfo>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="oigusNimi" type="xs:string" minOccurs="1">
                            <xs:annotation>
                              <xs:appinfo>
                                <xtee:title>Õiguse nimetus</xtee:title>
                              </xs:appinfo>

                            </xs:annotation>
                          </xs:element>
                          <xs:element name="oigusAlates" type="xs:dateTime" minOccurs="0">
                            <xs:annotation>
                              <xs:appinfo>
                                <xtee:title>Õigus kehtib alates</xtee:title>
                              </xs:appinfo>
                            </xs:annotation>

                          </xs:element>
                          <xs:element name="oigusKuni" type="xs:dateTime" minOccurs="0">
                            <xs:annotation>
                              <xs:appinfo>
                                <xtee:title>Õigus kehtib kuni</xtee:title>
                              </xs:appinfo>
                            </xs:annotation>
                          </xs:element>

                          <xs:element name="oigusPeatatud" type="xs:boolean" minOccurs="1">
                            <xs:annotation>
                              <xs:appinfo>
                                <xtee:title>Õigus on peatatud</xtee:title>
                              </xs:appinfo>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="oigusIkPiirang" type="xs:string" minOccurs="0">

                            <xs:annotation>
                              <xs:appinfo>
                                <xtee:title>Isikukoodi piirang</xtee:title>
                              </xs:appinfo>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="oigRegistrikood" type="xs:string" minOccurs="1">
                            <xs:annotation>

                              <xs:appinfo>
                                <xtee:title>Asutuse registrikood (õigus)</xtee:title>
                              </xs:appinfo>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="oigAsutusId" type="xs:integer" minOccurs="1">
                            <xs:annotation>
                              <xs:appinfo>

                                <xtee:title>Asutuse ID (õigus)</xtee:title>
                              </xs:appinfo>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="ametikohtId" type="xs:integer" minOccurs="1">
                            <xs:annotation>
                              <xs:appinfo>
                                <xtee:title>Ametikoha ID</xtee:title>

                              </xs:appinfo>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="akRegistrikood" type="xs:string" minOccurs="1">
                            <xs:annotation>
                              <xs:appinfo>
                                <xtee:title>Asutuse registrikood (ametikoht)</xtee:title>
                              </xs:appinfo>

                            </xs:annotation>
                          </xs:element>
                          <xs:element name="akAsutusId" type="xs:integer" minOccurs="1">
                            <xs:annotation>
                              <xs:appinfo>
                                <xtee:title>Asutuse ID (ametikoht)</xtee:title>
                              </xs:appinfo>
                            </xs:annotation>

                          </xs:element>
                        </xs:all>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:restriction>
              </xs:complexContent>
            </xs:complexType>
          </xs:element>

          <xs:element name="vastuseOsa" type="xs:boolean" minOccurs="1">
            <xs:annotation>
              <xs:appinfo>
                <xtee:title>Vastus on tagastatud osaliselt</xtee:title>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:all>

      </xs:complexType>

      <xs:complexType name="oigused3Paring">
        <xs:all>
          <xs:element name="oigused" type="xs:base64Binary" minOccurs="1">
            <xs:annotation>
              <xs:appinfo>
                <xtee:title>Asutuste nimekiri xml faili kujul</xtee:title>
              </xs:appinfo>

            </xs:annotation>
          </xs:element>
        </xs:all>
      </xs:complexType>

      <xs:complexType name="oigused3Vastus">
        <xs:all>
          <xs:element name="oigused" type="xs:base64Binary" minOccurs="1">
            <xs:annotation>

              <xs:appinfo>
                <xtee:title>Õiguste nimekiri xml faili kujul</xtee:title>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:all>
      </xs:complexType>

      <!-- Paringu manuse struktuur -->

      <xs:complexType name="oigused3ParingManus">
        <xs:all>
          <xs:element name="oigused" minOccurs="1" maxOccurs="1">
            <xs:complexType>
              <xs:all>
                <xs:element name="oigused" minOccurs="0" maxOccurs="1">
                  <xs:complexType>
                    <xs:complexContent>
                      <xs:restriction base="SOAP-ENC:Array">

                        <xs:sequence>
                          <xs:element name="oigus_nimi" type="xs:string" minOccurs="1" maxOccurs="unbounded" />
                        </xs:sequence>
                      </xs:restriction>
                    </xs:complexContent>
                  </xs:complexType>
                </xs:element>
                <xs:element name="asutused" minOccurs="0" maxOccurs="1">
                  <xs:complexType>

                    <xs:complexContent>
                      <xs:restriction base="SOAP-ENC:Array">
                        <xs:sequence>
                          <xs:element name="asutus_id" type="xs:integer" minOccurs="1" maxOccurs="unbounded" />
                        </xs:sequence>
                      </xs:restriction>
                    </xs:complexContent>
                  </xs:complexType>
                </xs:element>

                <xs:element name="ametikohad" minOccurs="0" maxOccurs="1">
                  <xs:complexType>
                    <xs:complexContent>
                      <xs:restriction base="SOAP-ENC:Array">
                        <xs:sequence>
                          <xs:element name="ametikoht_id" type="xs:integer" minOccurs="1" maxOccurs="unbounded" />
                        </xs:sequence>
                      </xs:restriction>
                    </xs:complexContent>

                  </xs:complexType>
                </xs:element>
                <xs:element name="OR" type="xs:boolean" default="false" minOccurs="0" maxOccurs="1" />
              </xs:all>
            </xs:complexType>
          </xs:element>
        </xs:all>
      </xs:complexType>

      <!-- Vastuse manuse struktuur -->
      <!-- Vastuses on tagastud ainult kehtivad õigused, ametikohad ning asutused.
           Vähemalt üks päringute massiividest peab olma mitte tühi.
           Juhul, kui päringus "OR" = true, siis vastused on tagastatud õigused mis vastavad reeglile:
           oigus = ANY (oigused) OR asutus = ANY (asutused) OR ametikoht = ANY (ametikohad)
           Juhul, kui päringus "OR" = false, siis vastused on tagastatud õigused mis vastavad reeglile:
           (oigused IS NULL OR oigus = ANY (oigused)) AND (asutused IS NULL OR asutus = ANY (asutused))
           AND (ametikohad IS NULL OR ametikoht IN (ametikohad)) -->
      <xs:complexType name="oigused3VastusManus">
        <xs:all>
          <xs:element name="oigused" minOccurs="1" maxOccurs="1">
            <xs:complexType>
              <xs:complexContent>
                <xs:restriction base="SOAP-ENC:Array">
                  <xs:sequence>

                    <xs:element name="oigus" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:all>
                          <xs:element name="oigus_id" type="xs:integer" minOccurs="1" maxOccurs="1" />
                          <xs:element name="oigus_nimi" type="xs:string" minOccurs="1" maxOccurs="1" />
                          <xs:element name="oigus_alates" type="xs:dateTime" minOccurs="0" maxOccurs="1" />
                          <xs:element name="oigus_kuni" type="xs:dateTime" minOccurs="0" maxOccurs="1" />
                          <xs:element name="oigus_ik_piirang" type="xs:string" minOccurs="0" maxOccurs="1" />
                          <xs:element name="asutus_id" type="xs:integer" minOccurs="1" maxOccurs="1" />

                          <xs:element name="asutus_nimetus" type="xs:string" minOccurs="1" maxOccurs="1" />
                          <xs:element name="asutus_nimetus_en" type="xs:string" minOccurs="0" maxOccurs="1" />
                          <xs:element name="asutus_nimetus_ru" type="xs:string" minOccurs="0" maxOccurs="1" />
                          <xs:element name="ametikoht_id" type="xs:integer" minOccurs="1" maxOccurs="1" />
                          <xs:element name="ametikoht_nimetus" type="xs:string" minOccurs="1" maxOccurs="1" />
                          <xs:element name="ametikoht_nimetus_en" type="xs:string" minOccurs="0" maxOccurs="1" />
                          <xs:element name="ametikoht_nimetus_ru" type="xs:string" minOccurs="0" maxOccurs="1" />
                          <xs:element name="taitja" minOccurs="0" maxOccurs="1">
                            <xs:complexType>

                              <xs:all>
                                <xs:element name="roll" type="xs:string" minOccurs="1" maxOccurs="1" />
                                <xs:element name="isikukood" type="xs:string" minOccurs="1" maxOccurs="1" />
                                <xs:element name="eesnimi" type="xs:string" minOccurs="1" maxOccurs="1" />
                                <xs:element name="perenimi" type="xs:string" minOccurs="1" maxOccurs="1" />
                                <xs:element name="telefon" type="xs:string" minOccurs="0" maxOccurs="1" />
                                <xs:element name="e_post" type="xs:string" minOccurs="0" maxOccurs="1" />
                              </xs:all>
                            </xs:complexType>

                          </xs:element>
                        </xs:all>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:restriction>
              </xs:complexContent>
            </xs:complexType>
          </xs:element>

        </xs:all>
      </xs:complexType>

      <xs:complexType name="asutusedParing">
        <xs:all>
          <xs:element name="asutused" type="xs:base64Binary" minOccurs="1">
            <xs:annotation>
              <xs:appinfo>
                <xtee:title>Asutuste nimekiri xml faili kujul</xtee:title>

              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:all>
      </xs:complexType>

      <xs:complexType name="asutusedVastus">
        <xs:all>
          <xs:element name="struktuur" type="xs:base64Binary" minOccurs="1">

            <xs:annotation>
              <xs:appinfo>
                <xtee:title>Asutuste struktuur xml faili kujul</xtee:title>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:all>
      </xs:complexType>

      <!-- Paringu manuse struktuur -->
      <xs:complexType name="asutusedParingManus">
        <xs:all>
          <xs:element name="asutused" minOccurs="1" maxOccurs="1">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="kood" type="xs:string" minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="id" type="xs:integer" minOccurs="0" maxOccurs="unbounded" />

                <xs:element name="nimetus" type="xs:string" minOccurs="0" maxOccurs="1" />
                <xs:element name="alamobjektideta" type="xs:boolean" minOccurs="0" maxOccurs="1" />
                <xs:element name="re_kood" type="xs:string" minOccurs="0" maxOccurs="1" />
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:all>
      </xs:complexType>

      <!-- Vastuse manuse struktuur -->
      <!-- Vastuses on toodud kõik asutused/ametikohad/täitmised: kehtivad, lõpetatud ning need mis kehtivad tulevikus.
           Kui vastuses on toodud asutuse asutamise_kp, siis asutus kehtib ainult sellest kuupäevast alates.
           Kui vastuses on toodud asutuse lopet_kp, siis asutus kehtib ainult enne seda kuupäeva.
           Kui vastuses on toodud ametikoha alates, siis ametikoht kehtib ainult sellest kuupäevast alates.
           Kui vastuses on toodud ametikoha kuni, siis ametikoht kehtib selle kuupäevani (kaasaarvatud).
           Kui vastuses on toodud täitmise alates, siis täitmine kehtib ainult sellest kuupäevast alates.
           Kui vastuses on toodud täitmise kuni, siis täitmine kehtib selle kuupäevani (kaasaarvatud).
