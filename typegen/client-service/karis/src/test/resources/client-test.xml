<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

  <import resource="classpath:client-test-common.xml" />

    <bean id="karisXTeeServiceImpl" class="ee.webmedia.xtee.client.karis.KarisXTeeServiceImpl">
		<property name="xTeeServiceConfigurationProvider" ref="karisXTeeServiceConfigurationProvider" />
		<property name="xTeeConsumer" ref="xTeeConsumer"/>
	</bean>
	

	<bean id="karisXTeeServiceConfigurationProvider"
		class="ee.webmedia.xtee.client.service.configuration.provider.PropertiesBasedXTeeServiceConfigurationProvider">
		<property name="resource" value="xtee-service.properties" />
	</bean>

	
	
	
</beans>
